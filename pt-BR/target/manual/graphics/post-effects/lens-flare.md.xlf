<source xml:lang="en">Lens flare</source>
<target xml:lang="pt">Flare de lente</target>
</trans-unit>
<trans-unit id="tu2" xml:space="preserve">
<source xml:lang="en"><x id="1"/>Intermediate<x id="2"/>
<x id="3"/>Artist<x id="4"/></source>
